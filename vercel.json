{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
         }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        },
        {
            "src": "/api/.*",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "dest": "/build/api/$1"
        }
    ]
  }